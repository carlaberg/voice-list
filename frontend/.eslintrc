{
  "extends": ["airbnb"],
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ],
    "max-len": ["error", 80],
    "quotes": [2, "single", "avoid-escape"],
    "semi": [2, "never"],
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "react/jsx-no-undef": 0,
    "eol-last": 0,
    "comma-dangle": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "browser": true
  }
}